{
  "paths" : {
    "source" : {
      "root": "./src",
      "html": "./src/**/*.html",
      "mustache": "./src/templates/",
      "data": "./src/templates/data.json",
      "js" : "./src/js",
      "images" : "./src/images/**/*",
      "fonts" : "./src/fonts",
      "css" : "./src/css/**/*.scss"
    },
    "public" : {
      "root" : "./public",
      "html" : "./public",
      "js" : "./public/js",
      "images" : "./public/images",
      "fonts" : "./public/fonts",
      "css" : "./public/css"
    }
  }
}

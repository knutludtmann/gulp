<!-- index.nunjucks -->
{% extends "layout.nunjucks" %}

{% block content %}
<h1>About</h1>



{% import 'macros/navigation.nunjucks' as nav %}

<!-- Creating the navigation with activePage = 'home' -->
{{nav.active('about')}}

<div class="gallery">
  <!-- Loops through "images" array -->
  {% for image in images %}
  <div class="gallery__item">
    <img src="{{image.src}}" alt="{{image.alt}}">
  </div>
  {% endfor %}
</div>

{% endblock %}